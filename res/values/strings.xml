    <!-- Application name, also used as label for main TimerSet activity -->
    <string name="app_label">Retro Timer</string>

    <!-- Activity labels -->
    <!-- Label for TimerAlert activity -->
    <string name="timer_alert_label">Retro Timer alert</string>
    <!-- Label for TimerSettings preferences activity -->
    <string name="timer_settings_label">Retro Timer settings</string>

    <!-- Notification texts -->
    <!-- Notification label, for when countdown is ongoing -->
    <string name="notify_set_label">Retro Timer is set</string>
    <!-- Notification text, for when countdown is ongoing -->
    <string name="notify_set_text">Alarm set to fire at <xliff:g id="time">%1$s</xliff:g></string>
    <!-- Notification label, for when alert is firing -->
    <string name="notify_triggered_label">Retro Timer alert</string>
    <!-- Notification text, for when alert is firing -->
    <string name="notify_triggered_text">Select to dismiss alarm</string>
    <!-- Notification label, for when alert is silenced (a.k.a. missed) -->
    <string name="notify_silenced_label">Retro Timer missed alarm</string>
    <!-- Notification text, for when alert is silenced (a.k.a. missed) -->
    <string name="notify_silenced_text">Alarm missed at <xliff:g id="time">%1$s</xliff:g></string>

    <!-- Labels -->
    <!-- Label shown over TimerAlert activity -->
    <string name="touch_to_dismiss">Touch to dismiss</string>
    <!-- Label sometimes shown over TimerSet activity -->
    <string name="volume_zero_warning">Note: Ringing is enabled, but alarm volume is zero so it will be silent! Go to settings to change this&#8230;</string>

    <!-- Menu labels -->
    <!-- Menu item for launching TimerSettings activity -->
    <string name="menu_settings">Settings</string>

    <!-- Preferences labels -->
    <!-- Title for preference to enable ringing on alart -->
    <string name="pref_ring_on_alarm_title">Ring</string>
    <!-- Summary for preference to enable ringing on alart -->
    <string name="pref_ring_on_alarm_summary">Ring when countdown reaches zero</string>
    <!-- Title for preference to enable vibrating on alart -->
    <string name="pref_vibrate_on_alarm_title">Vibrate</string>
    <!-- Summary for preference to enable vibrating on alart -->
    <string name="pref_vibrate_on_alarm_summary">Vibrate when countdown reaches zero</string>
    <!-- Title for preference to launch main system settings activity, to change alarm volume -->
    <string name="pref_go_to_volume_settings_title">Volume</string>
    <!-- Summary for preference to launch main system settings activity, to change alarm volume -->
    <string name="pref_go_to_volume_settings_summary">Go to system settings to change Alarm volume (a.k.a. Ringer volume)</string>

